{
  "$jason": {
    "head": {
      "data": {
        "posts": [
          {
            "type": "products",
            "items": [
              {
                "name": "LinkDood Communication",
                "location": "It's not only for communication",
                "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/linkcomm.png"
              },
              {
                "name": "IOT",
                "location": "Smart house to control with one touch of a button",
                "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/iot.png"
              },
              {
                "name": "Interconnected",
                "location": "Breaking communication barriers with IM-DNS, communicate with different platforms",
                "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/inter.png"
              },
              {
                "name": "Server",
                "location": "Use your own private server",
                "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/server.png"
              },
              {
                "name": "DDIO+SDK",
                "location": "Open Smart DDIO + SDK Development Platforms",
                "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/sdk.png"
              },
              {
                "name": "Secured",
                "location": "Triple End Encryption & Four Dimensional Protectio",
                "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/secured.png"
              }
            ]
          },
          {
            "name": "LinkDood Comm",
            "location": "It's not only for communication",
            "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/linkcomm.png"
          },
          {
            "name": "IOT",
            "location": "Smart house to control with one touch of a button",
            "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/iot.png"
          },
          {
            "name": "Interconnected",
            "location": "Breaking communication barriers with IM-DNS, communicate with different platforms",
            "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/inter.png"
          },
          {
            "name": "Server",
            "location": "Use your own private server",
            "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/server.png"
          },
          {
            "name": "DDIO+SDK",
            "location": "Open Smart DDIO + SDK Development Platforms",
            "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/sdk.png"
          },
          {
            "name": "Secured",
            "location": "Triple End Encryption & Four Dimensional Protectio",
            "content": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/secured.png"
          }
        ]
      },
      "templates": {
        "body": {
          "header": {
            "style": {
              "shy": "true"
            },
            "title": {
              "type": "image",
              "style": {
                "width": "94",
                "height": "27"
              },
              "url": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/linkdood.png"
            }
          },
          "sections": {
            "{{#each posts}}": [
              {
                "{{#if ('type' in this) && (type=='products') }}": {
                  "type": "vartical",
                  "items": {
                    "{{#each items}}": {
                      "type": "vertical"
                    }
                  }
                }
              },
              {
                "{{#else}}": {
                  "header": {
                    "type": "horizontal",
                    "style": {
                      "background": "#ffffff",
                      "padding": "9",
                      "spacing": "13"
                    },
                    "components": [
                      {
                        "type": "vertical",
                        "components": [
                          {
                            "type": "label",
                            "text": "{{name}}",
                            "style": {
                              "font": "HelveticaNeue-Bold",
                              "size": "24"
                            }
                          },
                          {
                            "type": "label",
                            "text": "{{location}}",
                            "style": {
                              "font": "HelveticaNeue",
                              "size": "16"
                            }
                          }
                        ]
                      }
                    ]
                  },
                  "items": [
                    {
                      "style": {
                        "width": "100%",
                        "padding": "0"
                      },
                      "type": "image",
                      "url": "{{content}}"
                    }
                  ]
                }
              }
            ]
          },
          "footer": {
            "tabs": {
              "style": {
                "background": "rgba(255,255,255,0.8)",
                "color": "#000000"
              },
              "items": [
                {
                  "type": "label",
                  "text": "Home",
                  "image": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/home.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/home.json"
                },
                {
                  "type": "label",
                  "text": "Product",
                  "image": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/product.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/search.json"
                },
                {
                  "type": "label",
                  "text": "About Us",
                  "image": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/about.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/camera.json"
                },
                {
                  "type": "label",
                  "text": "Contact Us",
                  "image": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/contact.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/heart.json"
                },
                {
                  "type": "label",
                  "text": "T&C",
                  "image": "https://raw.githubusercontent.com/daniellai9317/linkdood/master/images/tnc.png",
                  "style": {
                    "height": "21"
                  },
                  "url": "https://raw.githubusercontent.com/Jasonette/Instagram-UI-example/master/profile.json"
                }
              ]
            }
          }
        }
      }
    }
  }
}